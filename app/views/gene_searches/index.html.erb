<div class="row">
  <div class="col">
    <h1><%= dt("#{controller_name}.#{action_name}.title", model: dt("#{controller_name}.#{action_name}.model")) %></h1>
    <%= link_to GeneSearch.human_attribute_name(:destroy_all), delete_all_gene_searches_path, class: 'btn btn-danger', method: :delete, data: { confirm: dt("#{controller_name}.#{action_name}.confirm") } %>
  </div>
</div>
<div class="row">
  <table id="datatable_all" class="table table-striped table-bordered">
    <thead>
  	  <tr>
  	    <th><%= GeneSearch.human_attribute_name(:name) %></th>
  	  </tr>
    </thead>
    <tbody>
  		<% @gene_searches.each do |gene_search| %>
  		  <tr>
  		    <td>
  					<%= link_to content_tag(:span, "", class: "fa fa-trash-o"), gene_search_path(gene_search), method: :delete, class: "btn btn-mini btn-danger btn-xs", data: { confirm: dt("#{controller_name}.#{action_name}.confirm", model: gene_search.name) } %>
  					<%= link_to content_tag(:span, "", class: "fa fa-pencil"), edit_gene_search_path(gene_search), class: 'btn btn-small btn-primary btn-xs' %>
  					<%= link_to gene_search.name, gene_search_path(gene_search) %>
  				</td>
  		  </tr>
  		<% end %>
    </tbody>
  </table>
</div>
